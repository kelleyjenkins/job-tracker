<header>
<h2>All Companies</h2>
</header

<body>
<div class="company_list"
<ul>
  <% @companies.each do |company| %>
  <li class=company_<%= company.id %>>
    <h4><%= link_to company.name, company_path(company) %> </h4>
    <p><%= link_to "Edit", edit_company_path(company) %></p>
    <p><%= link_to "Delete", company_path(company), method: "delete" %></p>
  </li>
  <% end %>
</ul>
</div>
</body>
